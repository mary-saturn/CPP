}

//11
#include <iostream>
int main() {
int num;
num=14;
std::cout << num <<"\n";

char ch;
ch='a';
std::cout << ch <<"\n";

double d;
d=5.16;
std::cout << d << "\n";

float fl;
fl=3.786950;
std::cout << fl << "\n";

bool t_f;
t_f= 1;
std::cout << t_f <<"\n";

wchar_t w_t;
//w_t=; couldn't work this out (
std::cout << "show_must_go_on" << w_t <<"\n";
}

